  {
    "displayName": "Bartek dashboard for ${job}",
    "mosaicLayout": {
      "columns": 12,
      "tiles": [
        {
          "height": 4,
          "widget": {
            "title": "VM Memory usage for heap, ${job}.* [MAX]",
            "xyChart": {
              "chartOptions": {
                "mode": "COLOR"
              },
              "dataSets": [
                {
                  "minAlignmentPeriod": "60s",
                  "plotType": "LINE",
                  "timeSeriesQuery": {
                    "timeSeriesFilter": {
                      "aggregation": {
                        "alignmentPeriod": "60s",
                        "perSeriesAligner": "ALIGN_MAX"
                      },
                      "filter": "metric.type=\"agent.googleapis.com/jvm/memory/usage\" resource.type=\"gce_instance\" metric.label.\"memory_type\"=\"heap\" metadata.user_labels.\"dataflow_job_name\"=monitoring.regex.full_match(\"${job}.*\")"
                    }
                  }
                }
              ],
              "timeshiftDuration": "0s",
              "yAxis": {
                "label": "y1Axis",
                "scale": "LINEAR"
              }
            }
          },
          "width": 6
        },
        {
          "height": 4,
          "widget": {
            "title": "VM CPU usage for ${job}.* [MEAN]",
            "xyChart": {
              "chartOptions": {
                "mode": "COLOR"
              },
              "dataSets": [
                {
                  "minAlignmentPeriod": "60s",
                  "plotType": "LINE",
                  "timeSeriesQuery": {
                    "timeSeriesFilter": {
                      "aggregation": {
                        "alignmentPeriod": "60s",
                        "perSeriesAligner": "ALIGN_RATE"
                      },
                      "filter": "metric.type=\"compute.googleapis.com/instance/cpu/usage_time\" resource.type=\"gce_instance\" metadata.user_labels.\"dataflow_job_name\"=monitoring.regex.full_match(\"${job}.*\")",
                      "secondaryAggregation": {
                        "alignmentPeriod": "60s",
                        "perSeriesAligner": "ALIGN_MEAN"
                      }
                    }
                  }
                }
              ],
              "timeshiftDuration": "0s",
              "yAxis": {
                "label": "y1Axis",
                "scale": "LINEAR"
              }
            }
          },
          "width": 6,
          "xPos": 6
        },
        {
          "height": 4,
          "widget": {
            "title": "Element count for ${job}.*, MapElements/Map.out0 [MEAN], ",
